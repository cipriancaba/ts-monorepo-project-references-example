{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es6",
    "jsx": "preserve",
    "lib": ["es6"],
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
